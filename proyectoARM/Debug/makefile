-include ../makefile.defs

# Add inputs and outputs from these tool invocations to the build variables 
SECONDARY_FLASH += \
proyectoARM.hex \

SECONDARY_LIST += \
proyectoARM.lst \

SECONDARY_SIZE += \
proyectoARM.siz \


# All Target
all: proyectoARM.elf secondary-outputs

# Tool invocations
proyectoARM.elf: $(OBJS) $(USER_OBJS)
	@echo 'Building target: $@'
	@echo 'Invoking: ARM Sourcery Windows GCC C Linker'
	arm-none-eabi-gcc -T"C:\hlocal\EclipseARMwspace\proyectoARM\ram_ice.ld" -nostartfiles -Wl,-Map,proyectoARM.map -mcpu=arm7tdmi -g3 -gdwarf-2 -o "proyectoARM.elf" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo 'Finished building target: $@'
	@echo ' '
	$(MAKE) --no-print-directory post-build

proyectoARM.hex: proyectoARM.elf
	@echo 'Invoking: ARM Sourcery Windows GNU Create Flash Image'
	arm-none-eabi-objcopy -O binary proyectoARM.elf  "proyectoARM.hex"
	@echo 'Finished building: $@'
	@echo ' '

proyectoARM.lst: proyectoARM.elf
	@echo 'Invoking: ARM Sourcery Windows GNU Create Listing'
	arm-none-eabi-objdump -h -S proyectoARM.elf > "proyectoARM.lst"
	@echo 'Finished building: $@'
	@echo ' '

proyectoARM.siz: proyectoARM.elf
	@echo 'Invoking: ARM Sourcery Windows GNU Print Size'
	arm-none-eabi-size  --format=berkeley proyectoARM.elf
	@echo 'Finished building: $@'
	@echo ' '

# Other Targets
clean:
	-$(RM) $(SECONDARY_SIZE)$(OBJS)$(C_DEPS)$(ASM_DEPS)$(SECONDARY_FLASH)$(EXECUTABLES)$(SECONDARY_LIST)$(S_UPPER_DEPS) proyectoARM.elf
	-@echo ' '

post-build:
	-arm-none-eabi-objcopy -S -O binary "proyectoARM.elf" ""proyectoARM.bin"
	-@echo ' '

secondary-outputs: $(SECONDARY_FLASH) $(SECONDARY_LIST) $(SECONDARY_SIZE)

.PHONY: all clean dependents
.SECONDARY: post-build

-include ../makefile.targets
